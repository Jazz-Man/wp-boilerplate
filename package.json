  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jazz-Man/wp-boilerplate/issues"
  },
  "homepage": "https://github.com/Jazz-Man/wp-boilerplate#readme",
  "dependencies": {
    "bootstrap": "^4.4.1",
    "document-ready": "^2.0.2"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.3.2",
    "@types/document-ready": "^2.0.0",
    "@types/jquery": "^3.3.38",
    "@types/node": "^13.13.5",
    "@wordpress/base-styles": "^3.3.2",
    "@wordpress/block-library": "^2.17.0",
    "cross-env": "^7.0.2",
    "jquery": "^3.5.1",
    "laravel-mix": "^5.0.4",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "last 2 versions",
    "ie 6-8",
    "Firefox > 20"
  ]
}
