{
  "name": "bedrock",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "copy-block-styles": "node copy-block-styles.js",
    "development": "ENV=development WEBPACK_ARGS=\"--devtool source-map --output-pathinfo --debug\" npm run build",
    "watch": "ENV=development WEBPACK_ARGS=\"-w --devtool source-map --output-pathinfo --debug\" npm run build",
    "production": "WEBPACK_ARGS=\"-p\" npm run build",
    "build": "cross-env NODE_ENV=${ENV:-production} NODE_OPTIONS=--max_old_space_size=8192 node_modules/webpack/bin/webpack.js ${WEBPACK_ARGS} --progress --hide-modules --display-error-details --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jazz-Man/wp-boilerplate.git"
  },
