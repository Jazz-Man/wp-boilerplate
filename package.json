{
  "name": "bedrock",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "init": "cp node_modules/laravel-mix/setup/webpack.mix.js ./",
    "development": "cross-env process.env.section=website NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --devtool source-map --display-error-details --config=node_modules/laravel-mix/setup/webpack.config.js",
    "admin-development": "cross-env process.env.section=admin NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --devtool source-map --display-error-details --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env process.env.section=website NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --display-error-details --config=node_modules/laravel-mix/setup/webpack.config.js",
    "admin-watch": "cross-env process.env.section=admin NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --display-error-details --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env process.env.section=website NODE_ENV=production NODE_OPTIONS=--max_old_space_size=8192 node_modules/webpack/bin/webpack.js --progress --hide-modules --display-error-details --config=node_modules/laravel-mix/setup/webpack.config.js",
    "admin-production": "cross-env process.env.section=admin NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --display-error-details --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jazz-Man/wp-boilerplate.git"
  },
  "author": "vsokolyk <vsokolyk@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jazz-Man/wp-boilerplate/issues"
  },
  "homepage": "https://github.com/Jazz-Man/wp-boilerplate#readme",
  "dependencies": {
    "bootstrap": "^4.4.1",
    "document-ready": "^2.0.2"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.3.2",
    "@types/document-ready": "^2.0.0",
    "@types/jquery": "^3.3.38",
    "@types/node": "^13.13.5",
    "cross-env": "^7.0.2",
    "jquery": "^3.5.1",
    "laravel-mix": "^5.0.4"
  },
  "browserslist": [
    "last 2 versions",
    "ie 6-8",
    "Firefox > 20"
  ]
